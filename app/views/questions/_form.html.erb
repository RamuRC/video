<%= form_for @question do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :clip_id %><br />
    <%= f.text_field :clip_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :question %><br />
    <%= f.text_area :question %>
  </p>
  <p><%= f.submit %></p>
<% end %>
